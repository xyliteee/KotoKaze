        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:KotoKaze.Windows"
        xmlns:s ="clr-namespace:KotoKaze.Static" 
        mc:Ignorable="d"
        Title="GPUTestWindow" Height="720" Width="1280" Background="white">
    <Grid>
        <Canvas x:Name="outBase" Width="1280" Height="720">
            <Label x:Name="messageLable" Foreground ="Black" BorderThickness="0" Background="White" Opacity="1" Width="200" Height="50" Canvas.Bottom="0" FontSize="20"/>
        </Canvas>
    </Grid>
</Window>
